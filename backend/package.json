{
  "name": "ibp-monitor-frontend",
  "private": true,
  "version": "0.2.0/dev",
  "description": "IBP Monitor Frontend",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "nest build",
    "migrate": "ts-node ./node_modules/.bin/sequelize --config config/migrate.cjs db:migrate",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@chainsafe/libp2p-gossipsub": "^6.2.0",
    "@chainsafe/libp2p-noise": "^11.0.1",
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@libp2p/bootstrap": "^6.0.0",
    "@libp2p/kad-dht": "^8.0.5",
    "@libp2p/mdns": "^7.0.0",
    "@libp2p/mplex": "^8.0.1",
    "@libp2p/peer-id": "^2.0.3",
    "@libp2p/peer-id-factory": "^2.0.2",
    "@libp2p/peer-store": "^8.1.2",
    "@libp2p/pubsub-peer-discovery": "^8.0.0",
    "@libp2p/tcp": "^7.0.0",
    "@libp2p/websockets": "^6.0.0",
    "@nestjs/bull": "^0.6.3",
    "@nestjs/common": "^9.4.1",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.4.1",
    "@nestjs/event-emitter": "^1.4.1",
    "@nestjs/microservices": "^9.4.1",
    "@nestjs/platform-express": "^9.4.1",
    "@nestjs/platform-socket.io": "^9.4.2",
    "@nestjs/platform-ws": "^9.4.2",
    "@nestjs/schedule": "^2.2.2",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.4.2",
    "axios": "^1.4.0",
    "commander": "^10.0.1",
    "dotenv": "^16.0.3",
    "ioredis": "^5.3.2",
    "is-ip": "^5.0.0",
    "is-valid-hostname": "^1.0.2",
    "libp2p": "^0.44.0",
    "mariadb": "^2.5.6",
    "moment": "^2.29.4",
    "mysql2": "^3.3.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sequelize": "^6.31.1",
    "sequelize-typescript": "^2.1.5",
    "socket.io": "^4.6.2",
    "typeorm": "^0.3.16",
    "uint8arrays": "^4.0.3",
    "zeromq": "^5.3.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.3",
    "@types/sequelize": "^4.28.15",
    "@types/supertest": "^2.0.12",
    "@types/zeromq": "^5.2.2",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "run-script-webpack-plugin": "^0.2.0",
    "sequelize-cli": "^6.6.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "webpack": "^5.83.1",
    "webpack-cli": "^5.1.1",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
